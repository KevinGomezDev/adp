{
    "compilerOptions": {
        "module": "es6",
        "target": "es6",
        "sourceMap": true,
        "strict": true,
        "strictNullChecks": false,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "exclude": [
        "node_modules",
        ".git",
        ".vscode",
        "dist",
        "config"
    ]
}